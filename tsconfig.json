{
  "extends": "./node_modules/fastify-tsconfig/tsconfig.json",
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "module": "commonjs",
    "outDir": "dist",
    "target": "es2020",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts"]
}
